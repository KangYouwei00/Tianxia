narrative_event = {
	id = ko.487
	desc = EVTDESC_KO_328
	title = EVTTITLE_KO_328
	picture = GFX_evt_kgift

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_328
		
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	
	option = {
		name = EVTOPTB_KO_328
		trigger = { NOT = { dynasty = 1000078000 } }
		prestige = 30
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = { character_event = { id = ko.337 } }
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		} 
	}
	option = {
		name = EVTOPTC_KO_328 # Why is there an option that has the same trigger as others but that's just worse in every way?
		random_courtier = {
			limit = {
				age = 60
			}
			opinion = {
				modifier = opinion_ignorant
				who = ROOT
				years = 3
			}
		}
		if = {
			limit = { 
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		} 
	}
	option = {
		name = EVTOPTD_KO_328
		prestige = 20
		random_list = {
			20 = {
				change_learning = 1
			}
			80 = {}		
		}
		random_courtier = {
			opinion = {
				modifier = opinion_ignorant
				who = ROOT
				years = 3
			}
		} 
		if = {
			limit = { 
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
}	

