narrative_event = {
	id = ko.409
	desc = EVTDESC_KO_178
	title = EVTTITLE_KO_178	
	picture = GFX_evt_humiliation_prisoner

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_178
		prisoner_humiliation_effect = yes
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
			random_child = {
				random_list = {
					10 = {
						add_trait = cruel
					}
					90 = {}
				}
			} 	 		
		}
	}
	option = {
		remove_trait = kind
		name = EVTOPTB_KO_178
		
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			} 
		}
	}
	option = {
		add_trait = kind
		remove_trait = cruel
		name = EVTOPTC_KO_178
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		tooltip_info = lunatic
		trigger = { trait = lunatic }
		prestige = 500
		add_trait = kind
		add_character_modifier = {
			name = respected_by_the_people	
			duration = 365
		} 
		name = EVTOPTD_KO_178
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
}

