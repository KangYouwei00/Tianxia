	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_1013
		custom_tooltip = { text = EVTOPTA_KO_1013_TT }	 # This has no effect; why?
	}
}
		
# Lazy Family member makes it up the mountain
character_event = {
	id = ko.1074
	desc =  EVTDESC_KO_1014
	picture = GFX_evt_mountains
	is_triggered_only = yes

	option = {
		name = EVTOPTA_KO_1014
		custom_tooltip = { text = EVTOPTA_KO_1014_TT }		
 		random_list = {
			50 = {
				remove_trait = slothful
			}
			50 = {}
		}		
		hidden_tooltip = {
			any_courtier = {
				limit = {
					is_inaccessible_trigger = no
					opinion = { who = ROOT value = 10 }  
					family_of_root_trigger = yes
				} 
				opinion = {
					modifier = opinion_impressed
					who = ROOT
					years = 5
				}		
			}		
		}
	}
}