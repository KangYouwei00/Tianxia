					add_trait = paranoid
				}
			}
		}
		hidden_tooltip = {
			narrative_event = { id = ko.479 }
		}
	}
	option = {
		name = EVTOPTB_KO_189
		trigger = { religion = korean_pagan_reformed }	
		piety = 25
		any_courtier = { limit = { religion = buddhist }
			opinion = {
				modifier = opinion_charge_infidel # Is this the correct modifier?
				who = ROOT
				months = 6
			}
		}
		hidden_tooltip = {
			narrative_event = { id = ko.479 } 
		}		
	}
	option = {
		name = EVTOPTC_KO_189
		piety = -25
		add_trait = just
		any_courtier = { # Not random?
			limit = {
				trait = just
			} 
			opinion = {
				modifier = opinion_open_minded
				who = ROOT
				months = 6
			} 		
		}
		hidden_tooltip = {
			narrative_event = { id = ko.479 } 
		}		
	}							
}

