		name = EVTOPTA_KO_576.1 #Bring a little
		ai_chance = {
			factor = 1
			modifier = {
				factor = 2
				opinion = { who = from value = -40 }
			}
			modifier  = {
				factor = 2
				opinion = { who = from value = -20 }
			}
		}
		hidden_tooltip = { change_variable = { which = "mufeastlevel" value = 1 } }	
