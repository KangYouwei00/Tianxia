# Organize these events!
	
# Spooky Water Purification selector
character_event = {
	id = ko.700

	is_triggered_only = yes

	hide_window = yes

	immediate = {
		any_child  = {
			limit = {
				is_alive = yes
				has_minor_title = title_najeong
			}
			save_event_target_as = najeong		
		}
		random_list = {
			40 = {
				event_target:najeong = {
					character_event = { id = ko.709 } #bounce event
				}
			}
			10 = {
				narrative_event = { id = ko.702 }
			}
			15 = {
				narrative_event = { id = ko.706 }
			}
			15 = {
				narrative_event = { id = ko.708 }
			}
			15 = {
				narrative_event = { id = ko.316 }
			}
			5 = {
				narrative_event = { id = ko.317 }
			}
		}
	}
}	

# Bounce Event
character_event = {
	id = ko.709
	is_triggered_only = yes
	hide_window = yes
	
	option = {
		name = EVTOPTA_KO_254
		FROM = {
			narrative_event = { id = ko.701 }
		}	
	}
}


