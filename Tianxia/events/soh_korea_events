# Jungyangjeol
character_event = {
	id = ko.1069
	desc =  EVTDESC_KO_1009

	trigger = {
		# If this is triggered on_yearly_pulse/etc., some characters will never be able to get this event!
		month = 9
		NOT = { month = 10 }
	} 
	picture = GFX_evt_mountain_shrine
	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_1009
		any_courtier = {
			limit = {
				is_inaccessible_trigger = no 
				opinion = { who = ROOT value = 25 }  
				family_of_root_trigger = yes
			}
			# Nothing happens here; why?
		}
		# Are you meant to always get 2-3 events if you aren't injured/sick?
		hidden_tooltip = {
			if = {
				limit = {
					has_injury_trigger = yes
					has_disease_trigger = yes
				}
				character_event = { id = ko.1070 }
			}
		}
		hidden_tooltip = {
			if = {
				limit = {
					has_injury_trigger = no
					has_disease_trigger = no
				}
				character_event = { id = ko.1071 }
			}
		}
		hidden_tooltip = {
			if = {
				limit = {
					trait = slothful
				}
				character_event = { id = ko.1072 }
			}
		}
		custom_tooltip = { text = EVTOPTA_KO_1009_TT }
		piety = 50
		scaled_wealth = -0.1	
	}

	option = {
		name = EVTOPTB_KO_1009
		trigger = { event_target:korean_statuefatheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTB_KO_1009_TT }
		any_realm_character = {
			limit = {
				is_friend = ROOT
				opinion = {
					who = ROOT
					value = 90
				}
			}
			letter_event = { id = ko.1073 }
		} 
	}
		
	option = {
		name = EVTOPTC_KO_1009
		trigger = { event_target:korean_statuefatheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTB_KO_1009_TT }
		piety = 50
		scaled_wealth = -0.5		
	}
	option = {
		name = EVTOPTD_KO_1009 # This is the same as C
		trigger = { event_target:korean_statuefatheramidist  = { is_alive = no } }
		custom_tooltip = { text = EVTOPTD_KO_1009_TT }
		piety = 50
		scaled_wealth = -0.5		
	}	
}

