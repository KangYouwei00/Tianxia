				limit = {
				is_female = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			add_trait = fair # Congenial traits are basically never added post-birth; should it really be added here?
			}
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 } 
			}
		}
	}
	option = {
		name = EVTOPTC_KO_412 # This has no real effect; why?
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTD_KO_412
		any_child = {
			limit = {
				is_female = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			opinion = {
				modifier = opinion_open_minded
				who = ROOT
				years = 1
			}	
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
