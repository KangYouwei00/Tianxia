					NOT = { trait = incapable }
					NOT = { has_character_modifier = holding_large_feast }
					war = no
					is_adult = yes
				}
				letter_event = {
					id = ko.897 #invitations
					days = 2
				}
			}
		}
	}
	option = {
		name = EVTOPTB_KO_576
		ai_chance = {
