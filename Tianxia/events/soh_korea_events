			factor = 1.5
			trait = patient
		}
		modifier = {
			factor = 0.5
			trait = wroth
		}
		modifier = {
			factor = 0.5
			trait = proud
		}
		modifier = {
			factor = 0.5
			trait = humble
		}
		modifier = {
			factor = 0.5
			trait = gregarious
		}
		modifier = {
			factor = 0.5
			trait = paranoid
		}		
	}
	
	option = {
		name = EVTOPTA_KO_350.1 # A lot of stuff seems to go into a rather simple outcome; is this intentional?
		piety = 10
	}
}

