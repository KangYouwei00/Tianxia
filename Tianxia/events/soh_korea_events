# Calculating Approval Bounce Event
character_event = {
	id = ko.952

	is_triggered_only = yes

	hide_window = yes

	immediate = {
		random_list = {
			1 = {
				modifier = {
					factor = 1000
					is_variable_equal = { which = "mufeastlevel" value = 10 }
				}
				character_event = { id = ko.953 }
				any_realm_lord = {
					limit = {
						has_character_flag = coming_to_mu_ritual
					}
					character_event = { id = ko.953 }
				}
			} 
			1 = { 
				modifier = {
					factor = 1000
					is_variable_equal = { which = "mufeastlevel" value = 30 }
				}
				character_event = { id = ko.954 days = 1 }
				any_realm_lord = {
					limit = {
						has_character_flag = coming_to_mu_ritual
					}
					character_event = { id = ko.953 }
				}
			} 
		}
	}
} 

