# The ceremony is finished like
character_event = {
	id = ko.372
	desc = EVTDESC_KO_155
	picture = GFX_evt_mu

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_155
		piety = 25
		any_vassal = {
			limit = {
				OR = {
					is_republic = yes
					is_theocracy = yes
				}
			}
			opinion = {
				modifier = opinion_approves
				who = ROOT
				years = 5
			}
		}
		clr_character_flag = mu_in_the_house
		clr_character_flag = do_not_disturb		
	}
}

