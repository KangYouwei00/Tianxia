# The tale of Sim-Cheongga 5
narrative_event = {
	id = ko.1038
	desc = EVTDESC_KO_715
	title = EVTTITLE_KO_715
	picture = GFX_evt_hwarang_love

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_715
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_715
		random_child = {
			limit = {
				is_alive = yes
				age = 7
				NOR = {
					age = 16

				}
			}
			random_list = {
				25 = {
					add_trait = cynical
				}
				50 = {}
				25 = {
					add_trait = charitable
				} 
			}		
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTC_KO_715
		random_child = {
			limit = {
				is_female = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			add_trait = trusting		
		}
		random_child = {
			limit = {
				is_female = yes
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			opinion = {
				modifier = opinion_unimpressed
				who = ROOT
				years = 1
			}				
		}			
		trigger = { num_of_society_members = 0 } # Which society?
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
}

	
