# StoryHub
narrative_event = {
	id = ko.373
	desc = EVTDESC_KO_158
	title = EVTTITLE_KO_158
	picture = GFX_evt_mu

	is_triggered_only = yes
	
	option = { 				
		hidden_tooltip = { 
			if = { limit = { has_character_flag = ko.374 } narrative_event = { id = ko.374 }  }
			if = { limit = { has_character_flag = ko.393 } narrative_event = { id = ko.393 }  }
			if = { limit = { has_character_flag = ko.390 } narrative_event = { id = ko.390 }  }
			if = { limit = { has_character_flag = ko.397 } narrative_event = { id = ko.397 }  }
			if = { limit = { has_character_flag = ko.401 } narrative_event = { id = ko.401 }  }
			if = { limit = { has_character_flag = ko.404 } narrative_event = { id = ko.404 }  }
		}	
		name = {
            trigger = { has_character_flag = ko.374 }
            text = EVTOPTA_KO_158.11
        }
		name = {
            trigger = { has_character_flag = ko.393 }
            text = EVTOPTA_KO_158.12
        }
		name = {
            trigger = { AND = { has_character_flag = ko.390 dynasty = 1000078000  } }
            text = EVTOPTA_KO_158.13
        }
		name = {
            trigger = { AND = { has_character_flag = ko.390 NOT = { dynasty = 1000078000 }  } }
            text = EVTOPTA_KO_158.131
		}	
		name = {
            trigger = { AND = { has_character_flag = ko.390 dynasty = 1000155023  } }
            text = EVTOPTA_KO_158.14
        }
		name = {
            trigger = { AND = { has_character_flag = ko.390 NOT = { dynasty = 1000155023 }  } }
            text = EVTOPTA_KO_158.141
        }
		name = {
            trigger = { has_character_flag = ko.401 }
            text = EVTOPTA_KO_158.15
        }
		name = {
            trigger = { has_character_flag = ko.404 }
            text = EVTOPTA_KO_158.16
        }		
		
		
		any_courtier = {
			limit = { is_adult = no }
			random_list = {
				33 = { remove_trait = stressed }
				33 = { remove_trait = depressed }
				34 = {}
			}
			opinion = {
				modifier = opinion_favorite
 				who = ROOT
				years = 5
			} 
		}
	}
	option = {
		hidden_tooltip = { 
			if = { limit = { has_character_flag = ko.407 } narrative_event = { id = ko.407 }  } #slave story
			if = { limit = { has_character_flag = ko.410 } narrative_event = { id = ko.410 }  } #indian princess
			if = { limit = { has_character_flag = ko.414 } narrative_event = { id = ko.414 }  } #Seondeok
			if = { limit = { has_character_flag = ko.488 } narrative_event = { id = ko.488 }  } #yuri revenge
			if = { limit = { has_character_flag = ko.485 } narrative_event = { id = ko.485 }  } #talhae
			if = { limit = { has_character_flag = ko.480 } narrative_event = { id = ko.480 }  } #on go
		}			
		name = {
            trigger = { has_character_flag = ko.407 }
            text = EVTOPTB_KO_158.11
        }
		name = {
            trigger = { has_character_flag = ko.410 }
            text = EVTOPTB_KO_158.12
        }
		name = {
            trigger = { AND = { has_character_flag = ko.414 dynasty = 1000078000  } }
            text = EVTOPTB_KO_158.13
        }
		name = {
            trigger = { AND = { has_character_flag = ko.414 NOT = { dynasty = 1000078000 }  } }
            text = EVTOPTB_KO_158.131
		}	
		name = {
            trigger = {  has_character_flag = ko.488 dynasty = 1000155023  }
            text = EVTOPTB_KO_158.14
        }
		name = {
            trigger = {  has_character_flag = ko.488 NOT = { dynasty = 1000155023 }  } 
            text = EVTOPTB_KO_158.141
        }
		name = {
            trigger = { AND = { has_character_flag = ko.485 dynasty = 1000078000  } }
            text = EVTOPTB_KO_158.15
        }
		name = {
            trigger = { AND = { has_character_flag = ko.485 NOT = { dynasty = 1000078000 }  } }
            text = EVTOPTB_KO_158.151
		}
		name = {
            trigger = { has_character_flag = ko.480 }
            text = EVTOPTB_KO_158.16
        }
		any_courtier = {
			limit = { is_adult = no }
			random_list = {
				33 = { remove_trait = stressed }
				33 = { remove_trait = depressed }
				34 = {}
			}
			opinion = {
				modifier = opinion_favorite
				who = ROOT
				years = 5 
			} 		
		}
	}
	option = {
		hidden_tooltip = {
			if = { limit = { has_character_flag = ko.475 } narrative_event = { id = ko.475 }  } #Kim Yusin
			if = { limit = { has_character_flag = ko.648 } narrative_event = { id = ko.648 }  } #Rabbit Turtle
			if = { limit = { has_character_flag = ko.656 } narrative_event = { id = ko.656 }  } #Choson
			if = { limit = { has_character_flag = ko.682 } narrative_event = { id = ko.682 }  } #The Magic Drum
			if = { limit = { has_character_flag = ko.728 } narrative_event = { id = ko.728 }  } #The Death of Wangbi U
			if = { limit = { has_character_flag = ko.732 } narrative_event = { id = ko.732 }  } #The Story of Silk
		}			
		name = {
            trigger = { has_character_flag = ko.475 }
            text = EVTOPTC_KO_158.11
        }
		name = {
            trigger = { has_character_flag = ko.648 }
            text = EVTOPTC_KO_158.12
        }
		name = {
            trigger = { has_character_flag = ko.656 }
            text = EVTOPTC_KO_158.13
        }
		name = {
