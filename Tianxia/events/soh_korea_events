	id = ko.473
	desc = EVTDESC_KO_353
	picture = GFX_evt_mu

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_353
		add_trait = proud
		if = {
			limit = {
				has_dlc = "Reapers"
			}
			add_trait = physician
		}
	}
	option = {
		name = EVTOPTB_KO_353
		piety = 30
		if = {
			limit = {
				has_dlc = "Reapers"
			}
			add_trait = physician
		}
		
	}
	option = {
		name = EVTOPTC_KO_353 # This event option does nothing without Reaper's Due and is objectively worse regardless of whether you have Reaper's Due; this seems very strange...
		if = {
			limit = {
				has_dlc = "Reapers"
			}
			add_trait = physician
		}
	}
}
	
