# The Tale of Momju 3
narrative_event = {
	id = ko.1025
	desc = EVTDESC_KO_702
	title = EVTTITLE_KO_702
	picture = GFX_evt_monk_eastern_group

	is_triggered_only = yes
	
	option = {
		name = EVTOPTA_KO_702
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
	}
	option = {
		name = EVTOPTB_KO_702
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
				random_child = {
					limit = {
						is_female = yes
						is_alive = yes
						age = 6
						NOR = {
							age = 16
							trait = cynical
							trait = weak
						}
					}
					change_intrigue = 1
				}
			}
		}
	}	
	option = {
		name = EVTOPTC_KO_702
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}
		random_child = {
			limit = {
				is_alive = yes
				age = 6
				NOR = {
					age = 16
					trait = cynical
					trait = weak
				}
			}
			add_trait = curious	 # This is a Conclave childhood trait, but there is no check for Conclave!
		}			
	}
	option = {
		name = EVTOPTD_KO_702
		trigger = { has_dlc = "Reapers" }
		hidden_tooltip = {
			character_event = { id = RIP.21000 days = 30 }
		}
		if = {
			limit = {
				has_character_flag = mu_in_the_house
			}
			hidden_tooltip = {
				character_event = { id = ko.337 }
			}
		}
		if = {
			limit = {
				has_character_flag = story_pend
			}
			hidden_tooltip = {
				narrative_event = { id = ko.383 }
			}
		}			
	}	
}

