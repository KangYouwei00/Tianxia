		}
		hidden_tooltip = { 
			if = {
				limit = {
					has_character_flag = mu_in_the_house
				}
				hidden_tooltip = {
					character_event = { id = ko.337 }
				}
			}
			if = {
				limit = {
					has_character_flag = story_pend
				}
				hidden_tooltip = {
					narrative_event = { id = ko.383 }
				}
			}
		}
	}
	option = { 
		name = EVTOPTC_KO_182
